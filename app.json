{
  "expo": {
    "name": "personalfit-ai-app",
    "slug": "personalfit-ai-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.perryhoang2012.personalfitaiapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.perryhoang2012.personalfitaiapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },

    "fonts": [
      {
        "asset": "./assets/fonts/Inter-Black.ttf",
        "name": "Black"
      },
      {
        "asset": "./assets/fonts/Inter-BlackItalic.ttf",
        "name": "BlackItalic"
      },
      {
        "asset": "./assets/fonts/Inter-Bold.ttf",
        "name": "Bold"
      },
      {
        "asset": "./assets/fonts/Inter-BoldItalic.ttf",
        "name": "BoldItalic"
      },
      {
        "asset": "./assets/fonts/Inter-ExtraBold.ttf",
        "name": "ExtraBold"
      },
      {
        "asset": "./assets/fonts/Inter-ExtraBoldItalic.ttf",
        "name": "ExtraBoldItalic"
      },
      {
        "asset": "./assets/fonts/Inter-ExtraLight.ttf",
        "name": "ExtraLight"
      },
      {
        "asset": "./assets/fonts/Inter-ExtraLightItalic.ttf",
        "name": "ExtraLightItalic"
      },
      {
        "asset": "./assets/fonts/Inter-Italic.ttf",
        "name": "Italic"
      },
      {
        "asset": "./assets/fonts/Inter-Light.ttf",
        "name": "Light"
      },
      {
        "asset": "./assets/fonts/Inter-LightItalic.ttf",
        "name": "LightItalic"
      },
      {
        "asset": "./assets/fonts/Inter-Medium.ttf",
        "name": "Medium"
      },
      {
        "asset": "./assets/fonts/Inter-MediumItalic.ttf",
        "name": "MediumItalic"
      },
      {
        "asset": "./assets/fonts/Inter-Regular.ttf",
        "name": "Regular"
      },
      {
        "asset": "./assets/fonts/Inter-SemiBold.ttf",
        "name": "SemiBold"
      },
      {
        "asset": "./assets/fonts/Inter-SemiBoldItalic.ttf",
        "name": "SemiBoldItalic"
      },
      {
        "asset": "./assets/fonts/Inter-Thin.ttf",
        "name": "Thin"
      },
      {
        "asset": "./assets/fonts/Inter-ThinItalic.ttf",
        "name": "ThinItalic"
      }
    ]
  }
}
